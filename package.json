  "bugs": {
    "url": "https://github.com/CaliaCode/rockpaperscissors/issues"
  },
  "homepage": "https://github.com/CaliaCode/rockpaperscissors#readme",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.2"
  }
